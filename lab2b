int sensorValue = 0;
int outputValue = 0;
int analogOutPin = 9;
int analogPin = A0;

void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  
}

void loop() {
  // put your main code here, to run repeatedly:
  sensorValue = analogRead(analogPin);
  outputValue =map(sensorValue, 0,1023,0,255);
  analogWrite(analogOutPin, outputValue);
  Serial.println("Input value:" +sensorValue);
  Serial.println("OutputValue:" +outputValue);
  delay(2);
}
